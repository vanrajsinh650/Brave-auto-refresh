{
  "manifest_version": 3,
  "name": "Pro Tab Refresher",
  "version": "2.0",
  "description": "Refresh any selected tab automatically",
  "permissions": ["tabs", "storage", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Pro Tab Refresher"
  }
}
